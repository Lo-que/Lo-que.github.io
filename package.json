  "main": "index.ts",
  "scripts": {
    "javascript": "browserify -p [ tsify --noImplicitAny ] src/index.ts > build/index.js",
    "html": "minify src/index.html > build/index.html",
    "css": "minify src/index.css > build/index.css",
    "images": "imagemin src/assets build",
    "copy": "cp -r src/assets build",
    "clear": "rm -rf build",
    "build": "npm run clear && npm run javascript && npm run html && npm run css",
    "start": "serve build"
  },
