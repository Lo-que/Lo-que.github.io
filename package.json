    "browserify": "^17.0.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "imagemin-cli": "^7.0.0",
    "minify": "^8.0.4",
    "prettier": "^2.6.2",
    "serve": "^13.0.2",
    "tsify": "^5.0.4",
    "typescript": "^4.6.3"
  }
